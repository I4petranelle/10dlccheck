{
  "cleanUrls": true,
  "trailingSlash": false,

  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.10dlccheck.com" }],
      "destination": "https://10dlccheck.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "10dlccheck.vercel.app" }],
      "destination": "https://10dlccheck.com/:path*",
      "permanent": true
    },
    {
      "source": "/learn/ctia-guidelines-explained.html",
      "destination": "/learn/ctia-guidelines-explained",
      "permanent": true
    },
    {
      "source": "/learn/sms-compliance-checklist.html",
      "destination": "/learn/sms-compliance-checklist",
      "permanent": true
    },
    {
      "source": "/learn/what-is-10dlc.html",
      "destination": "/learn/what-is-10dlc",
      "permanent": true
    },
    {
      "source": "/learn/index.html",
      "destination": "/learn/",
      "permanent": true
    }
  ],

  "rewrites": [
    {
      "source": "/extension/help",
      "destination": "/extension/index.html"
    },
    {
      "source": "/extension/help/",
      "destination": "/extension/index.html"
    }
  ],

  "headers": [
    {
      "source": "/partials/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ]
}
