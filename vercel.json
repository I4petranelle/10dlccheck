{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.10dlccheck.com" }],
      "destination": "https://10dlccheck.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "10dlccheck.vercel.app" }],
      "destination": "https://10dlccheck.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Strict-Transport-Security", "value": "max-age=63072000; includeSubDomains; preload" }
      ]
    },
    {
      "source": "/partials/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ]
}

